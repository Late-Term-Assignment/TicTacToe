{
  "name": "late_term_assignment",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe built using continuous delivery",
  "private": true,
  "scripts": {
    "test": "jest --verbose --coverage",
    "watch": "jest --watchAll",
    "build": "webpack",
    "heroku-postbuild": "webpack -p",
    "start": "node app.js",
    "dev-server": "webpack-dev-server --open",
    "serverTest": "jest --config config.json && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build-test": "npm run build && npm run test && npm run serverTest"
  },
  "keywords": [
    "Tic-Tac-Toe"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Late-Term-Assignment/TicTacToe.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "node-fetch": "^2.2.0"
  }
}
