{
  "name": "late_term_assignment",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe built using continuous delivery",
  "private": true,
  "scripts": {
    "test": "jest --verbose --coverage",
    "build": "webpack --open --config webpack.prod.js",
    "build-test": "npm run build && npm run test",
    "heroku-postbuild": "webpack -p --config webpack.dev.js",
    "start": "node app.js",
    "dev-client": "webpack-dev-server --config webpack.dev.js && npm run start",
    "dev-server": "nodemon ./app.js localhost:3000",
    "create-docs": "node markdown-pdf.js",
    "server-test": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "Tic-Tac-Toe"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Late-Term-Assignment/TicTacToe.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "markdown-pdf": "^9.0.0",
    "node-loader": "^0.6.0",
    "nodemon": "^1.18.5",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "express": "^4.16.4",
    "fetch": "^1.1.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.2.0",
    "puppeteer": "^1.9.0"
  }
}
